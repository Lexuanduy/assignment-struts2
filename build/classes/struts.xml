<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <constant name="struts.devMode" value="true" />
    <package name="default" namespace="/create" extends="struts-default">
        <action name="createEvent"
                class="fa.edu.action.CreateEventAction"
                method="execute">
            <result name="CREATE">/create-event.jsp</result>
        </action>
        <action name="listEvent"
                class="fa.edu.action.ListEventAction"
                method="execute">
            <result name="LIST">/list-event.jsp</result>
        </action>
    </package>
</struts>
